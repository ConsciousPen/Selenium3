<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="E2E Scenario 8" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>

    <test name="HSS Scenario 1 E2E with DP3" parallel="none">
        <parameter name="state" value="UT, AZ, NJ, OK" />
        <classes>
            <class name="aaa.modules.e2e.home_ss.dp3.TestScenario8" />
        </classes>
    </test>

    <test name="HSS Scenario 1 E2E with HO3" parallel="none">
        <parameter name="state" value="UT, AZ, NJ, OK" />
        <classes>
            <class name="aaa.modules.e2e.home_ss.ho3.TestScenario8" />
        </classes>
    </test>

    <test name="HSS Scenario 1 E2E with HO4" parallel="none">
        <parameter name="state" value="UT, AZ, NJ, OK" />
        <classes>
            <class name="aaa.modules.e2e.home_ss.ho4.TestScenario8" />
        </classes>
    </test>

    <test name="HSS Scenario 1 E2E with HO6" parallel="none">
        <parameter name="state" value="UT, AZ, NJ, OK" />
        <classes>
            <class name="aaa.modules.e2e.home_ss.ho6.TestScenario8" />
        </classes>
    </test>
</suite>
