<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="AAA Docgen Scenarios" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>
    <test name="Preconditions" preserve-order="true">
        <classes>
            <class name="aaa.modules.preconditions.CleanAsyncTasks"/>
            <class name="aaa.modules.preconditions.CleanCache"/>
            <class name="aaa.modules.preconditions.CleanCacheAdmin"/>
            <class name="aaa.modules.preconditions.DocGenPreconditions"/>
            <class name="aaa.modules.preconditions.ClearFolders"/>
        </classes>
    </test>
</suite>
