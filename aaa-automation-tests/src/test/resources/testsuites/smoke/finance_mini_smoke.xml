<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Finance Mini Smoke Test" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>

    <test name="Smoke Test - SS PUP">
        <parameter name="state" value="AZ"/>
        <classes>
            <class name="aaa.modules.financials.pup.TestNewBusiness">
                <methods>
                    <include name="testNewBusinessScenario_1"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Smoke Test - SS Auto">
        <parameter name="state" value="AZ"/>
        <classes>
            <class name="aaa.modules.financials.auto_ss.TestNewBusiness">
                <methods>
                    <include name="testNewBusinessScenario_1"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Smoke Test - SS HO3">
        <parameter name="state" value="AZ"/>
        <classes>
            <class name="aaa.modules.financials.home_ss.ho3.TestNewBusiness">
                <methods>
                    <include name="testNewBusinessScenario_1"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Smoke Test - CA PUP">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.financials.pup.TestNewBusiness">
                <methods>
                    <include name="testNewBusinessScenario_1"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Smoke Test - CA Choice">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.financials.auto_ca.choice.TestNewBusiness">
                <methods>
                    <include name="testNewBusinessScenario_1"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Smoke Test - CA Select">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.financials.auto_ca.select.TestNewBusiness">
                <methods>
                    <include name="testNewBusinessScenario_1"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Smoke Test - CA HO3">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.financials.home_ca.ho3.TestNewBusiness">
                <methods>
                    <include name="testNewBusinessScenario_1"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>
