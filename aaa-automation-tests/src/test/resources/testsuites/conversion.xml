<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Conversion Test" verbose="1" parallel="methods" thread-count="1024">
	<listeners>
		<!--<listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>-->
		<listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener" />
	</listeners>
	<parameter name="timeshift-scenario-mode" value="method" />

	<test name="Conversion Test" preserve-order="true">
		<parameter name="state" value="CA" />
		<classes>
			<class name="aaa.modules.conversion.HdesConversionTest" />
			<class name="aaa.modules.conversion.SisConversionTest" />
			<class name="aaa.modules.conversion.FoxProConversionTest" />
		</classes>
	</test>

	<test name="MAIG Conversion Test" preserve-order="true">
		<classes>
			<class name="aaa.modules.conversion.MaigConversionTest" />
		</classes>
	</test>

</suite>
