<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Batch Test Parallel suite" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener2"/>
        <listener class-name="com.exigen.istf.exec.testng.TimeShiftTestNgListener"/>
    </listeners>

    <test name="Batch Jobs in Parallel"  parallel="methods">
        <parameter name="state" value="UT"/>
        <classes>
            <class name="aaa.modules.bct.batch.BatchTestParallel"/>
        </classes>
    </test>

</suite>