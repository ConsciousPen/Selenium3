<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="BCT Renewals Online Tests" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener" />
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test" />
    <!-- Renewal -->

    <test name="Renewal - BCT_ONL_048" preserve-order="true">
        <parameter name="state" value="CA" />
        <classes>
            <class name="aaa.modules.bct.renewal.DoNotRenewTest">
                <methods>
                    <include name="BCT_ONL_048_NonRenewal"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Renewal - BCT_ONL_107" preserve-order="true">
        <parameter name="state" value="NJ" />
        <classes>
            <class name="aaa.modules.bct.renewal.RenewalTest">
                <methods>
                    <include name="BCT_ONL_107_ManualChangesRenewal"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>