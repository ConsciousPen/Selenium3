<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Product Factory" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>
    <suite-files>
        <suite-file path="productfactory_with_timeshift.xml"/>
        <suite-file path="productfactory_without_timeshift.xml"/>
    </suite-files>
</suite>
