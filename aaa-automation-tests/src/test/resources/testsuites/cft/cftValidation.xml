<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="CFT Validation" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>

	<test name="Test Preconditions" preserve-order="true">
		<classes>
   			<class name="aaa.modules.preconditions.SetTodayDate"/>
	 	</classes>
	</test>

    <test name="CFT Validation" preserve-order="true">
		<classes>
            <class name="aaa.modules.cft.TestCFTValidator" />
        </classes>
    </test>

</suite>
