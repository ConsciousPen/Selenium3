<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="AAA CFT Scenarios" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>

    <suite-files>
        <suite-file path="./Scenario1.xml" />
        <suite-file path="./Scenario2.xml" />
        <suite-file path="./Scenario3.xml" />
        <suite-file path="./Scenario4.xml" />
        <suite-file path="./Scenario5.xml" />
        <suite-file path="./Scenario6.xml" />
        <suite-file path="./Scenario7.xml" />
        <suite-file path="./Scenario8.xml" />
    </suite-files>

</suite>
