<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="CFT Scenario 2" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>

    <test name="AutoSS Scenario 2 CFT" parallel="none">
        <parameter name="state" value="AZ, CA, KY, MD" />
        <classes>
            <class name="aaa.modules.cft.auto_ss.TestCFTScenario2" />
        </classes>
    </test>

    <test name="HomeSS Scenario 2 CFT" parallel="none">
        <parameter name="state" value="DE, NJ, OK, UT, CT, KS, KY, OH" />
        <classes>
            <class name="aaa.modules.cft.home_ss.ho3.TestCFTScenario2" />
        </classes>
    </test>

</suite>
