<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Financials Earned Premiums Test Suite" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>


    <!--New Business Tests-->


    <test name="New Business - SS">
        <parameter name="state" value="AZ"/>
        <classes>
            <class name="aaa.modules.financials.pup.TestNewBusiness"/>
            <class name="aaa.modules.financials.auto_ss.TestNewBusiness"/>
            <class name="aaa.modules.financials.home_ss.dp3.TestNewBusiness"/>
            <class name="aaa.modules.financials.home_ss.ho3.TestNewBusiness"/>
            <class name="aaa.modules.financials.home_ss.ho4.TestNewBusiness"/>
            <class name="aaa.modules.financials.home_ss.ho6.TestNewBusiness"/>
        </classes>
    </test>

    <test name="New Business - CA">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.financials.auto_ca.choice.TestNewBusiness"/>
            <class name="aaa.modules.financials.auto_ca.select.TestNewBusiness"/>
            <class name="aaa.modules.financials.home_ca.dp3.TestNewBusiness"/>
            <class name="aaa.modules.financials.home_ca.ho3.TestNewBusiness"/>
            <class name="aaa.modules.financials.home_ca.ho4.TestNewBusiness"/>
            <class name="aaa.modules.financials.home_ca.ho6.TestNewBusiness"/>
        </classes>
    </test>

</suite>