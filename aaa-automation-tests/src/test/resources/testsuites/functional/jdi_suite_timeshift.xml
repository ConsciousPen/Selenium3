<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="JDI Time Shift Test Suite" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <!-- Listener commented out for local runs-->
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>

    <test name="TestEValueMembershipProcessJobAdding"  parallel="methods" thread-count="1024">
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestEValueMembershipProcess">
                <methods>
                    <include name="precondJobAdding"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="TestInstallmentFees" parallel="methods" thread-count="1024">
        <classes>
            <class name="aaa.modules.regression.billing_and_payments.auto_ss.functional.TestInstallmentFees"/>
        </classes>
    </test>
    <test name="TestEndorsementActionRules" parallel="methods" thread-count="1024">
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestEndorsementActionRules"/>
            <class name="aaa.modules.regression.sales.auto_ca.select.functional.TestEndorsementActionRules"/>
        </classes>
    </test>

    <test name="pas3697_membershipEligibilityConfigurationFalseForActiveMembership" parallel="methods" thread-count="1024">
        <parameter name="state" value="VA"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestEValueMembershipProcess">
                <methods>
                    <include name="retrieveMembershipSummaryEndpointCheck"/>
                    <include name="pas3697_membershipEligibilityConfigurationFalseForActiveMembership"/>
                    <include name="pas3697_membershipEligibilityConfigurationFalseForCancelledMembershipRenewal"/>
                    <include name="pas3697_membershipEligibilityConfigurationFalseForNotActiveMembership"/>
                    <include name="pas3697_membershipEligibilityConfigurationFalseForPendingMembership"/>
                    <include name="pas3697_membershipEligibilityConfigurationTrueForActiveMembership"/>
                    <include name="pas3697_membershipEligibilityConfigurationTrueForCancelledMembershipRenewal"/>
                    <include name="pas3697_membershipEligibilityConfigurationTrueForNotActiveMembership"/>
                    <include name="pas3697_membershipEligibilityConfigurationTrueForPendingMembership"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="pas5837_eValueDiscountRemovedIfPaperlessPreferenceIsPending" parallel="methods" thread-count="1024">
        <parameter name="state" value="DC"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestEValueMembershipProcess">
                <methods>
                    <include name="retrieveMembershipSummaryEndpointCheck"/>
                    <include name="pas5837_eValueDiscountRemovedIfPaperlessPreferenceIsPending"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="HO pas7039_newDataElementsDeceasedIndicator" parallel="methods" thread-count="1024">
        <parameter name="state" value="VA"/>
        <classes>
            <class name="aaa.modules.regression.billing_and_payments.home_ss.ho3.functional.TestRefundProcess">
                <methods>
                    <include name="pas7039_newDataElementsDeceasedNo"/>
                    <include name="pas7039_newDataElementsDeceasedYes"/>
                </methods>
            </class>
        </classes>
    </test>


</suite>