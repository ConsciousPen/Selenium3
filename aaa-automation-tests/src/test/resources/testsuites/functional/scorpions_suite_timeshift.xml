<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Scorpions Functional Test Suite Timeshift" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>

        <test name="DP3 pas9114 Pre Renewal Print Sequence">
        <parameter name="state" value="PA" />
        <classes>
            <class name="aaa.modules.regression.document_fulfillment.home_ss.dp3.functional.TestMaigSpecificFormsGeneration" >
                <methods>
                    <include name="pas9114_PreRenewalPrintSequenceMortgagee"/>
                    <include name="pas9114_PreRenewalPrintSequence"/>
                </methods>
            </class>
        </classes>
    </test>
    <!-- Auto SS -->
    <test name="Update VIN Refresh Auto SS" >
        <parameter name="state" value="UT"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestVINUpload">
                <methods>
                    <include name="pas2716_AutomatedRenewal_ExpirationDate"/>
                    <include name="pas2716_AutomatedRenewal_ExpirationDateMinus35"/>
                    <include name="pas2716_AutomatedRenewal_ExpirationDateMinus45"/>
                    <include name="pas527_UpdatedVinRenewal"/>
                    <include name="pas11659_Renewal_VersionR46"/>
                    <include name="pas11659_Renewal_VersionR45"/>
                    <include name="pas11659_Renewal_VersionR35"/>
                    <include name="pas11659_Renewal_VersionR25"/>
                    <include name="pas12872_VINRefreshNoMatchRenewalTimeline"/>
                    <include name="pas12872_VINRefreshPartialtoFullMatchRenewaltimeline"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestMSRPRefreshMotorHomeVehicle">
                <methods>
                    <include name="pas730_RenewalVehicleTypeNotPPA"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestMSRPRefreshMotorHomeVehicle">
                <methods>
                    <include name="pas730_RenewalVehicleTypePPA"/>
                    <include name="pas730_RenewalVINDoesMatchNBandNoMatchOnRenewal"/>
                </methods>
            </class>
        </classes>
    </test>

    <!-- Auto Ca Select-->

    <test name="Update VIN Refresh CA Select" >
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ca.select.functional.TestVINUpload">
                <methods>
                    <include name="pas2716_AutomatedRenewal_ExpirationDate"/>
                    <include name="pas2716_AutomatedRenewal_ExpirationDateMinus35"/>
                    <include name="pas2716_AutomatedRenewal_ExpirationDateMinus45"/>
                    <include name="pas527_UpdatedVinRenewal"/>
                    <include name="pas12872_VINRefreshNoMatchOnRenewalAutoCA"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.sales.auto_ca.select.functional.TestMSRPRefreshNotRegularVehicle">
                <methods>
                    <include name="pas730_RenewalVehicleTypeNotRegular"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.sales.auto_ca.select.functional.TestMSRPRefreshRegularVehicle">
                <methods>
                    <include name="pas730_RenewalVehicleTypeRegular"/>
                    <include name="pas730_RenewalVINDoesMatchNBandNoMatchOnRenewal"/>
                </methods>
            </class>
        </classes>
    </test>

    <!-- Auto Choice -->

    <test name="Update VIN Refresh CA Choice" >
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ca.choice.functional.TestVINUpload">
                <methods>
                    <include name="pas2716_AutomatedRenewal_ExpirationDate"/>
                    <include name="pas2716_AutomatedRenewal_ExpirationDateMinus35"/>
                    <include name="pas2716_AutomatedRenewal_ExpirationDateMinus45"/>

                    <include name="pas527_UpdatedVinRenewal"/>
                    <include name="pas12872_VINRefreshNoMatchOnRenewalAutoCA"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.sales.auto_ca.choice.functional.TestMSRPRefreshNotRegularVehicle">
                <methods>
                    <include name="pas730_RenewalVehicleTypeNotRegular"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.sales.auto_ca.choice.functional.TestMSRPRefreshRegularVehicle">
                <methods>
                    <include name="pas730_RenewalVehicleTypeRegular"/>
                    <include name="pas730_RenewalVINDoesMatchNBandNoMatchOnRenewal"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>
