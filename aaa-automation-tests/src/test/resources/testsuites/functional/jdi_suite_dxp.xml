<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="JDI DXP Test Suite" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <!-- Listener commented out for local runs-->
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>

    <test name="GreenButtonServiceAZ" parallel="methods" thread-count="1024">
        <parameter name="state" value="AZ"/>
        <classes>
            <class name="aaa.modules.regression.service.auto_ss.functional.TestMiniServicesNonPremiumBearing">
                <methods>
                    <include name="pas9997_paymentMethodsLookup"/>
                    <include name="pas9997_paymentPlansLookup"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="PolicySummaryService AZ" parallel="methods" thread-count="1024">
        <parameter name="state" value="AZ"/>
        <classes>
            <class name="aaa.modules.regression.service.auto_ss.functional.TestMiniServicesNonPremiumBearing">
                <methods>
                    <include name="pas9716_policySummaryForPolicy"/>
                    <include name="pas9716_policySummaryForLapsedRenewal"/>
                    <include name="pas9716_policySummaryForConversionManual"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="PolicySummaryService VA" parallel="methods" thread-count="1024">
        <parameter name="state" value="VA"/>
        <classes>
            <class name="aaa.modules.regression.service.auto_ss.functional.TestMiniServicesNonPremiumBearing">
                <methods>
                    <include name="pas9716_policySummaryForActiveRenewal"/>
                    <include name="pas9716_policySummaryForConversion"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="PolicySummaryService CA" parallel="methods" thread-count="1024">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.service.auto_ca.choice.functional.TestMiniServicesNonPremiumBearing">
                <methods>
                    <include name="pas9716_policySummaryForPolicy"/>
                    <include name="pas9716_policySummaryForActiveRenewal"/>
                    <include name="pas9716_policySummaryForLapsedRenewal"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>