        </classes>
    </test>

    <test name="TestNonPremiumBearingEndorsementsSS">
        <parameter name="state" value="AZ, CO, CT, DC, DE, ID, IN, KS, KY, MD, MT, NJ, NV, NY, OH, OK, OR, PA, SD, UT, VA, WV, WY" />
        <classes>
            <class name="aaa.modules.regression.service.auto_ss.functional.TestMiniServicesNonPremiumBearing"/>
        </classes>
    </test>
</suite>