<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="JDI Functional Test Suite" verbose="1" parallel="false" thread-count="1">

    <listeners>
        <!-- Listener commented out for local runs-->
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
     </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>

    <test name="TestRefundProcess">
        <classes>
            <class name="aaa.modules.regression.billing_and_payments.auto_ss.functional.TestRefundProcess"/>
