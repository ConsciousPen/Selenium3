    </test>
    <test name="TestPaperlessPreferences">
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestPaperlessPreferences"/>
        </classes>
    </test>
    <test name="TestTriggersAH35XX">
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestTriggersAH35XX"/>
        </classes>
    </test>
    <test name="TestInstallmentFees">
        <classes>
            <class name="aaa.modules.regression.billing_and_payments.auto_ss.functional.TestInstallmentFees"/>
