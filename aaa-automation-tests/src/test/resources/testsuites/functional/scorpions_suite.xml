<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Scorpions Functional Test Suite" verbose="1" parallel="none" thread-count="1024">
    <listeners>
        <!-- Listener commented out for local runs-->
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>
    <test name="PAS-541 Auto SS Symbols in Policy Details Service">
        <parameter name="state" value="AZ"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestGetAutoPolicyDetails"/>
        </classes>
    </test>
    <test name="PAS-3794,PAS-3795 New Business NJ_DE: Non-Member Message and validation error">
        <parameter name="state" value="DE, NJ"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestMembershipValidationError"/>
        </classes>
    </test>
    <test name="PAS-2247, PAS-2248 - Lock Membership and Auto Ins Persistency, Not at Fault and Comp Claims">
        <parameter name="state" value="CT"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestAutoPoliciesLock"/>
        </classes>
    </test>
    <test name="Auto SS TestVINUpload tests">
        <parameter name="state" value="UT"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestVINUpload">
                <methods>
                    <include name="pas6203_VinAndControlTablesUpload"/>
                    <include name="pas533_newVinAdded"/>
                    <include name="pas527_UpdatedVinRenewal"/>
                    <include name="pas2714_Endorsement"/>
                    <include name="pas4253_restrictVehicleRefreshNB"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestMSRPRefreshMotorHomeVehicle">
                <methods>
                    <include name="pas730_VehicleTypeNotPPA"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestMSRPRefreshPPAVehicle">
                <methods>
                    <include name="pas730_PartialMatch"/>
                    <include name="pas730_VehicleTypePPA"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Auto Select TestVINUpload tests">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ca.select.functional.TestVINUpload">
                <methods>
                    <include name="pas533_newVinAdded"/>
                    <include name="pas527_UpdatedVinRenewal"/>
                    <include name="pas2714_Endorsement"/>
                    <include name="pas4253_restrictVehicleRefreshNB"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.sales.auto_ca.select.functional.TestMSRPRefreshNotRegularVehicle">
                <methods>
                    <include name="pas730_VehicleTypeNotRegular"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.sales.auto_ca.select.functional.TestMSRPRefreshRegularVehicle">
                <methods>
                    <include name="pas730_PartialMatch"/>
                    <include name="pas730_VehicleTypeRegular"/>
                    <include name="pas730_vinDoesNotMatchDB"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Auto Choice TestVINUpload tests">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ca.choice.functional.TestVINUpload">
                <methods>
                    <include name="pas533_newVinAdded"/>
                    <include name="pas527_updatedVinRenewal"/>
                    <include name="pas2714_Endorsement"/>
                    <include name="pas4253_restrictVehicleRefreshNB"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.sales.auto_ca.choice.functional.TestMSRPRefreshRegularVehicle">
                <methods>
                    <include name="pas730_PartialMatch"/>
                    <include name="pas730_VehicleTypeRegular"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.sales.auto_ca.choice.functional.TestMSRPRefreshNotRegularVehicle">
                <methods>
                    <include name="pas730_VehicleTypeNotRegular"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="pas2704_DeclarationPage AA02NY">
        <parameter name="state" value="NY"/>
        <classes>
            <class name="aaa.modules.regression.document_fulfillment.auto_ss.TestSymbolsPresenceDeclarationPage">
                <methods>
                    <include name="pas2704_DeclarationPage"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
