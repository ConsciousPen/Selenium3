<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="CIO Full Test Suite" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>
    <suite-files>
        <suite-file path="./cio_membership_suite.xml"/>
        <suite-file path="./cio_suite.xml"/>
        <suite-file path="./cio_timeshift.xml"/>
    </suite-files>
</suite>
