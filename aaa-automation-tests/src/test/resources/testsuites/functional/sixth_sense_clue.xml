        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>

    <test name="Test CLUE Simplification for CA DP3" parallel="methods" thread-count="1024">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.sales.home_ca.dp3.functional.TestClueSimplification">
                <methods>
                    <exclude name="pas6759_AbilityToRemoveManuallyEnteredClaimsRenewal"/>
                    <exclude name="pas21557_RequireUWRuleCATIndicatorIncludeInRatingAndEligibilityFieldsAreChanged"/>
