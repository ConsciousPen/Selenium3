<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Sixth Sense Functional Test Suite" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>

    <test name="Test Enhanced UIM Coverage Forms">
        <parameter name="state" value="MD"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestEUIMForms"/>
        </classes>
    </test>

    <test name="Test Enhanced UIM Coverage Behavior">
        <parameter name="state" value="MD"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestEUIMCoverageBehavior"/>
        </classes>
    </test>

    <test name="Test Nano Enhanced UIM Coverage Behavior">
        <parameter name="state" value="MD"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestMDNanoPolicyEUIMBehavior"/>
        </classes>
    </test>

</suite>
