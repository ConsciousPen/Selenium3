    <parameter name="timeshift-scenario-mode" value="method"/>

    <test name="New Business 1/2 - SS PUP" parallel="methods" thread-count="1024">
        <parameter name="state" value="KY, WV"/>
        <classes>
            <class name="aaa.modules.financials.pup.TestNewBusiness">
