<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Show Me The Money Time Shift Test Suite" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method" />
    <test name="CA TestFinancePolicyEscheatmentCheckReversals" parallel="methods" thread-count="1024">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.finance.billing.home_ca.ho3.TestFinancePolicyEscheatmentCheckReversals"/>
        </classes>
    </test>
    <test name="CA TestFinanceCheckPermissionForReversal" parallel="methods" thread-count="1024">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.finance.billing.home_ca.ho3.TestFinanceCheckPermissionForReversal"/>
        </classes>
    </test>
    <test name="CA TestFinanceWaiveInstallmentFeeTC1A" parallel="methods" thread-count="1024">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.finance.billing.home_ca.ho3.TestFinanceSmallBalanceWriteOffOnLastInstallmentBill_PaidInFull"/>
        </classes>
    </test>
    <test name="CA TestFinanceWaiveInstallmentFeeTC1B" parallel="methods" thread-count="1024">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.finance.billing.auto_ss.TestFinanceSmallBalanceWriteOffOnLastInstallmentBill_Overpaid"/>
        </classes>
    </test>
    <test name="KY TestFinanceWaiveInstallmentFeeTC1C" parallel="methods" thread-count="1024">
        <parameter name="state" value="KY"/>
        <classes>
            <class name="aaa.modules.regression.finance.billing.auto_ss.TestFinanceSmallBalanceWriteOffOnLastInstallmentBill_Underpaid"/>
        </classes>
    </test>
    <test name="WV TestFinanceWaiveInstallmentFeeTC1C" parallel="methods" thread-count="1024">
        <parameter name="state" value="WV"/>
        <classes>
            <class name="aaa.modules.regression.finance.billing.home_ss.ho3.TestFinanceSmallBalanceWriteOffOnLastInstallmentBill_Underpaid"/>
        </classes>
    </test>
    <test name="AZ TestFinanceWaiveInstallmentFeeTC1D" parallel="methods" thread-count="1024">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.finance.billing.home_ss.ho4.TestFinanceSmallBalanceWriteOffOnLastInstallmentBill_Underpaid"/>
        </classes>
    </test>
    <test name="NJ TestFinanceWaiveInstallmentFeeTC1D" parallel="methods" thread-count="1024">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.finance.billing.home_ss.ho6.TestFinanceSmallBalanceWriteOffOnLastInstallmentBill_Underpaid"/>
        </classes>
    </test>
    <!-- Earned Premium Calculation Changes: PAS-14510-->
    <test name="PAS-14510 Earned Premium Calculation Changes - I scenario" parallel="methods" thread-count="1024">
        <parameter name="state" value="AZ, KY, WV, NJ"/>
        <classes>
            <class name="aaa.modules.regression.finance.ledger.auto_ss.TestFinanceEPCalculationOOSEndorsement"/>
        </classes>
    </test>
    <test name="PAS-20300 Earned Premium Calculation Changes - Cancel Flag" parallel="methods" thread-count="1024">
        <parameter name="state" value="PA"/>
        <classes>
            <class name="aaa.modules.regression.finance.ledger.home_ss.ho3.TestFinanceEPCalculationCancelFlagTransactions"/>
        </classes>
    </test>
    <test name="PAS-14510 Earned Premium Calculation Changes - II,III,V scenarios" parallel="methods" thread-count="1024">
        <parameter name="state" value="AZ, KY, WV"/>
        <classes>
            <class name="aaa.modules.regression.finance.ledger.auto_ss.TestFinanceEPCalculationOOSEndorseCancelReinstate"/>
            <class name="aaa.modules.regression.finance.ledger.auto_ss.TestFinanceEPCalculationOOSCancelAndReinstate"/>
