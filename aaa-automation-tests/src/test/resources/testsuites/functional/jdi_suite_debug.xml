<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="JDI Functional Test Suite" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <!-- Listener commented out for local runs-->
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>

     <test name="TestRefundProcessVA">
        <parameter name="state" value="VA"/>
        <classes>
            <class name="aaa.modules.regression.billing_and_payments.auto_ss.functional.TestRefundProcess">
                <methods>
                    <include name="precondJobAdding"/>
                    <include name="refundDocumentGenerationConfigCheck"/>
                    <include name="eRefundLastPaymentMethodConfigCheck"/>
                    <include name="pendingRefundPaymentMethodConfigCheck"/>
                    <include name="pas2186_RefundProcessCheck"/>
                    <include name="pas352_RefundMethodAndDropdownLastPaymentMethodCreditCard"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="TestRefundProcessAZ">
        <parameter name="state" value="AZ"/>
        <classes>
            <class name="aaa.modules.regression.billing_and_payments.auto_ss.functional.TestRefundProcess">
                <methods>
                    <include name="eRefundLastPaymentMethodConfigCheck"/>
                    <include name="pas352_RefundMethodAndDropdownLastPaymentMethodDebitCardMc"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="TestRefundProcessMD">
        <parameter name="state" value="MD"/>
        <classes>
            <class name="aaa.modules.regression.billing_and_payments.auto_ss.functional.TestRefundProcess">
                <methods>
                    <include name="eRefundLastPaymentMethodConfigCheck"/>
                    <include name="pas352_RefundMethodAndDropdownLastPaymentMethodEFT"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="TestEValueDiscountVA">
        <parameter name="state" value="VA"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestEValueDiscount">
                <methods>
                    <include name="eValueAcknowledgementConfigCheck"/>
                    <include name="eValueConfigCheck"/>
                    <include name="eValueMembershipConfigCheck"/>
                    <include name="eValuePriorBiCurrentBiConfigCheck"/>
                    <include name="eValueTerritoryChannelForVAConfigCheck"/>
                    <include name="paperlessPreferencesConfigCheck"/>
                    <include name="precondJobAdding"/>

                    <include name="pas436_eValueDiscountVariations"/>
                    <include name="pas3693_eValueConfiguration"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>