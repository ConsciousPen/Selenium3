<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Financials Validation Test Suite - Renewal" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>

    <test name="Renewal 1/2 - SS PUP" parallel="methods" thread-count="1024">
        <parameter name="state" value="KY, WV"/>
        <classes>
            <class name="aaa.modules.financials.pup.TestRenewal">
                <methods>
                    <include name="testRenewalScenario_1"/>
                    <include name="testRenewalScenario_2"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Renewal 3/4 - SS PUP" parallel="methods" thread-count="1024">
        <parameter name="state" value="AZ"/>
        <classes>
            <class name="aaa.modules.financials.pup.TestRenewal">
                <methods>
                    <exclude name="testRenewalScenario_1"/>
                    <exclude name="testRenewalScenario_2"/>
                </methods>
            </class>
        </classes>
    </test>

