<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Sixth Sense Functional Time Shift Test Suite" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>

    <!--Tests for Auto-->

    <test name="Test Calculate Tier for NY converted policies" parallel="methods" thread-count="1024">
        <parameter name="state" value="NY"/>
        <classes>
            <class name="aaa.modules.regression.conversions.auto_ss.functional.TestTierCalculation">
                <methods>
                    <include name="pas2443_calculateTierNyConversionCheckNB"/>
                    <include name="pas2443_calculateTierNyConversionCheckRenewal"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Test Reinstatement point score lock during mid term transaction" parallel="methods" thread-count="1024">
        <parameter name="state" value="NJ"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestMidTermReinstatementPointLock">
                <methods>
                    <include name="pas9687_MidTermReinstatementPointsLocked"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Test Enhanced UIM Coverage Forms" parallel="methods" thread-count="1024">
        <parameter name="state" value="MD"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestEUIMForms">
                <methods>
                    <include name="pas11302_testEUIMMDFormNB"/>
                    <include name="pas11302_testEUIMMDFormConversion"/>
                    <include name="pas11302_testEUIMMDFormEndorsement"/>
                    <include name="pas11302_testEUIMMDFormRenewal"/>
                    <include name="pas12466_AC1_NB"/>
                    <include name="pas12466_AC2_Endorsement"/>
                    <include name="pas12466_AC3_Conversion"/>
                    <include name="pas12466_AC3_Renewal"/>
                    <include name="pas12466_AC3_Revised_Renewal_After_Renewal_Term_Change"/>
                    <include name="pas12466_AC3_Revised_Renewal_After_Current_Term_Change"/>
                    <include name="pas12466_do_not_generate_AAEUIMMD_if_coverage_removed"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>