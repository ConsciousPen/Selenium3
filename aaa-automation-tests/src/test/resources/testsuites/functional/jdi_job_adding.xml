<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="JDI Job Adding" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <!-- Listener commented out for local runs-->
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>

    <test thread-count="1024" name="TestEValueDiscountJobAdding" parallel="methods">
        <parameter name="state" value="AZ"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestEValueDiscount">
                <methods>
                    <include name="precondJobAdding"/>
                </methods>
            </class>
        </classes>
    </test>
    <test thread-count="1024" name="TestEValueMembershipProcessJobAdding" parallel="methods">
        <parameter name="state" value="AZ"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ss.functional.TestEValueMembershipProcess">
                <methods>
                    <include name="precondJobAdding"/>
                </methods>
            </class>
        </classes>
    </test>
    <test thread-count="1024" name="TestRefundProcessJobAdding" parallel="methods">
        <parameter name="state" value="AZ"/>
        <classes>
            <class name="aaa.modules.regression.billing_and_payments.auto_ss.functional.TestRefundProcess">
                <methods>
                    <include name="precondJobAdding"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
