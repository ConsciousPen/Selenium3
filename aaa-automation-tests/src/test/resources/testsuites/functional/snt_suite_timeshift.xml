<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="SnT Time Shift Test Suite" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <!-- Listener commented out for local runs-->
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>

    <test name="HSRNHODPXX/HSRNMXX form (AZ, NJ) HO3" parallel="methods" thread-count="1024">
        <parameter name="state" value="AZ, NJ"/>
        <classes>
            <class name="aaa.modules.regression.document_fulfillment.home_ss.ho3.functional.TestMaigConversionHomeHO3">
                <methods>
                    <include name="pas2309_renewalCoverLetterHSRNHODPXX"/>
                    <include name="pas2309_renewalCoverLetterPupConvHSRNHODPXX"/>
                    <include name="pas2309_renewalCoverLetterPupHSRNHODPXX"/>
                    <include name="pas2570_renewalCoverLetterHSRNMXX"/>
                    <include name="pas2570_renewalCoverLetterPupConvHSRNMXX"/>
                    <include name="pas2570_renewalCoverLetterPupHSRNMXX"/>
                </methods>
            </class>
            <class name="aaa.modules.regression.document_fulfillment.pup.TestMaigConversionPup">
                <methods>
                    <include name="pas2571_renewalCoverLetterHSRNPUPXXHO3"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="HSRNHODPXX form (AZ, VA) HO4" parallel="methods" thread-count="1024">
