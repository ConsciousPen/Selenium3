<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="refresh renewal merge suite" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <!-- Listener commented out for local runs-->
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="test"/>
    
    <test name="Auto CA Refresh Renewal Merge Tests">
        <parameter name="state" value="CA"/>
        <classes>
            <class name="aaa.modules.regression.sales.auto_ca.select.functional.TestCurrentTermEndAddsVehicle">
                <methods>
                    <include name="pas14532_refreshForCurrentAndRenewalTermsVinMatched"/>
                </methods>
            </class>
        </classes>
    </test>
  </suite>
