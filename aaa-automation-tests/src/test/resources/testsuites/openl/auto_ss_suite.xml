<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Auto SS OpenL Rating Tests" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <!--<listener class-name="aaa.helpers.listeners.AlterSuiteListener"/>-->
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>
    <parameter name="testsDir" value="src/test/resources/openl/auto_ss"/>

    <test name="AutoSS [AZ] Premium Calculation Test" preserve-order="true">
        <parameter name="state" value="AZ"/>
        <parameter name="fileName" value="AZTests-20171101.xls"/>
