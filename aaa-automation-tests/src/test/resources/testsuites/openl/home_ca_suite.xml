<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Home CA OpenL Rating Tests" verbose="1" parallel="methods" thread-count="1024">
    <listeners>
        <!--<listener class-name="aaa.helpers.listeners.AlterSuiteListener"/> -->
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>
    <parameter name="testsDir" value="src/test/resources/openl/home_ca"/>

    <test name="Home CA DP3 Premium Calculation Test" preserve-order="true">
        <parameter name="state" value="CA"/>
        <parameter name="fileName" value="DP3Tests_eff_03012018.xls"/>
        <parameter name="policyNumbers" value="1"/>
        <classes>
            <class name="aaa.modules.openl.HomeCaDP3PremiumCalculationTest"/>
        </classes>
    </test>

    <test name="Home CA HO3 Premium Calculation Test" preserve-order="true">
        <parameter name="state" value="CA"/>
        <parameter name="fileName" value="HO3Tests_eff_09142017.xls"/>
        <parameter name="policyNumbers" value="1"/>
        <classes>
            <class name="aaa.modules.openl.HomeCaHO3PremiumCalculationTest"/>
        </classes>
    </test>

    <test name="Home CA HO4 Premium Calculation Test" preserve-order="true">
        <parameter name="state" value="CA"/>
        <parameter name="fileName" value="HO4Tests_eff_01012018.xls"/>
        <parameter name="policyNumbers" value="1"/>
        <classes>
            <class name="aaa.modules.openl.HomeCaHO4PremiumCalculationTest"/>
        </classes>
    </test>

    <test name="Home CA HO6 Premium Calculation Test" preserve-order="true">
        <parameter name="state" value="CA"/>
        <parameter name="fileName" value="HO6Tests_eff_01012018.xls"/>
        <parameter name="policyNumbers" value="1"/>
        <classes>
            <class name="aaa.modules.openl.HomeCaHO6PremiumCalculationTest"/>
        </classes>
    </test>
</suite>
