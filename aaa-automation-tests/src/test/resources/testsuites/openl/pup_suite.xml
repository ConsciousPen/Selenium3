<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="PUP OpenL Rating Tests" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <!--<listener class-name="aaa.helpers.listeners.AlterSuiteListener"/> -->
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>
    <parameter name="testsDir" value="src/test/resources/openl/pup"/>
    <test name="PUP [AZ] Premium Calculation Test" preserve-order="true">
        <parameter name="state" value="AZ"/>
        <parameter name="fileName" value="AZTests-20170920.xls"/>
        <parameter name="policyNumbers" value="41,139"/>
        <classes>
            <class name="aaa.modules.openl.PUPPremiumCalculationTest"/>
        </classes>
    </test>
    <test name="PUP [UT] Premium Calculation Test" preserve-order="true">
        <parameter name="state" value="UT"/>
        <parameter name="fileName" value="UTTests-20150808.xls"/>
        <parameter name="policyNumbers" value="23,69"/>
        <classes>
            <class name="aaa.modules.openl.PUPPremiumCalculationTest"/>
        </classes>
    </test>
    <test name="PUP [MT] Premium Calculation Test" preserve-order="true">
        <parameter name="state" value="MT"/>
        <parameter name="fileName" value="MTTests.xls"/>
        <parameter name="policyNumbers" value="246,282"/>
        <classes>
            <class name="aaa.modules.openl.PUPPremiumCalculationTest"/>
        </classes>
    </test>
    <test name="PUP [NV] Premium Calculation Test" preserve-order="true">
        <parameter name="state" value="NV"/>
        <parameter name="fileName" value="NVTests-20170522.xls"/>
        <parameter name="policyNumbers" value="7,318"/>
        <classes>
            <class name="aaa.modules.openl.PUPPremiumCalculationTest"/>
        </classes>
    </test>
</suite>
