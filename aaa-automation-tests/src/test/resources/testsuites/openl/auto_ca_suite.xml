<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Auto CA OpenL Rating Tests" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <!--<listener class-name="aaa.helpers.listeners.AlterSuiteListener"/> -->
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>
    <parameter name="testsDir" value="src/test/resources/openl/auto_ca"/>

    <test name="Auto CA Choice Premium Calculation Test" preserve-order="true">
        <parameter name="state" value="CA"/>
        <parameter name="fileName" value="TestsCAChoiceTest-20170806.xls"/>
        <parameter name="policyNumbers" value="1,3"/>
        <classes>
            <class name="aaa.modules.openl.AutoCaChoicePremiumCalculationTest"/>
        </classes>
    </test>

    <!--<test name="Auto CA Select Premium Calculation Test" preserve-order="true">
        <parameter name="state" value="CA"/>
        <parameter name="fileName" value="TestsCASelectTest.xls"/>
        <parameter name="policyNumbers" value="1"/>
        <classes>
            <class name="aaa.modules.openl.AutoCaSelectPremiumCalculationTest"/>
        </classes>
    </test>-->
</suite>
