<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression Tests" verbose="1" parallel="tests" thread-count="1024">
    <listeners>
        <listener class-name="aaa.helpers.listeners.AlterSuiteListener2"/>
        <listener class-name="com.exigen.istf.exec.testng.impl.TimeShiftTestNGListener"/>
    </listeners>
    <parameter name="timeshift-scenario-mode" value="method"/>
    <suite-files>
        <suite-file path="regression_auto.xml"/>
        <suite-file path="regression_property.xml"/>
    </suite-files>
</suite>
