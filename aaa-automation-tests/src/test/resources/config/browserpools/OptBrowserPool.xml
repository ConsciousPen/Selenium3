<browsers>
    <properties>
        <name>WEBDRIVER</name>
        <seleniumType>WEBDRIVER</seleniumType>
        <browserProfile>${browser.pool.browsertype}</browserProfile>
        <useXpathLibrary>true</useXpathLibrary>
        <allowNativeXPath>true</allowNativeXPath>
        <browserLifetime>0</browserLifetime>
        <timeout>200000</timeout>
        <capability name="unexpectedAlertBehaviour">ignore</capability>
    </properties>
    <hub>
        <!-- <url>http://aws2aaaqas24.corevelocity.csaa.cloud:9999</url>-->
        <url>${browser.pool.hub}</url>
        <min>${browser.pool.threads}</min>
        <max>${browser.pool.threads}</max>
        <keywords>
            <keyword>${browser.pool.keyword}</keyword>
        </keywords>
    </hub>
</browsers>