<browsers>
	<properties>
		<name>WEBDRIVER</name>
		<seleniumType>WEBDRIVER</seleniumType>
		<browserProfile>firefox</browserProfile>
		<useXpathLibrary>true</useXpathLibrary>
		<allowNativeXPath>true</allowNativeXPath>
		<browserLifetime>0</browserLifetime>
		<timeout>200000</timeout>
		<capability name="unexpectedAlertBehaviour">ignore</capability>
	</properties>
	
	<hub>
		<url>http://aws2aaaqas53.corevelocity.csaa.cloud:9999</url>
		<min>2</min>
		<max>2</max>
		<keywords>
			<keyword>CSAA</keyword>
		</keywords>
	</hub>
	
</browsers>