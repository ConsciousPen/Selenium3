<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.exigen.etcs</groupId>
	<artifactId>aaa</artifactId>
	<version>1.2-SNAPSHOT</version>
	<name>AAA-ETCSA parent</name>
	<packaging>pom</packaging>

	<modules>
		<module>aaa-automation-ui</module>
		<module>aaa-automation-tests</module>
	</modules>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<!-- <project.version>1.0-SNAPSHOT</project.version> -->
		<maven.compiler.version>3.3</maven.compiler.version>
		<!--<product>ALL</product> -->

	</properties>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>${maven.compiler.version}</version>
				<configuration>
					<encoding>${project.build.sourceEncoding}</encoding>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-javadoc-plugin</artifactId>
				<version>2.10.3</version>
				<configuration>
					<show>private</show>
					<nohelp>true</nohelp>
					<excludePackageNames>*.helpers.*:*.utils.*:*BaseTest*:*.scenarios.*:*.compatibility.*</excludePackageNames>
					<doclet>com.exigen.ipb.etcsa.utils.javadoc.DocLet</doclet>
					<docletArtifact>
						<groupId>com.exigen.eis</groupId>
						<artifactId>eis-automation</artifactId>
						<version>0.1</version>
					</docletArtifact>
					<!--<additionalparam>-product ${product}</additionalparam> -->
					<useStandardDocletOptions>false</useStandardDocletOptions>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<dependencies>
		<dependency>
			<groupId>org.seleniumhq.selenium</groupId>
			<artifactId>selenium-server</artifactId>
			<version>3.4.0</version>
		</dependency>
		<dependency>
			<groupId>org.testng</groupId>
			<artifactId>testng</artifactId>
			<version>6.9.11</version>
		</dependency>

		<!-- EIS BASE CORE -->
		<dependency>
			<groupId>com.exigen.eis</groupId>
			<artifactId>etcs-automation-core</artifactId>
			<version>8.9-M1</version>
		</dependency>
		<!-- EIS BASE CORE END -->
		<!-- ISTF Artifacts START -->
		<dependency>
			<groupId>com.exigen.istf</groupId>
			<artifactId>istf-core</artifactId>
			<version>3.4.1</version>
		</dependency>
		<dependency>
			<groupId>com.exigen.istf</groupId>
			<artifactId>test-analytics-listener</artifactId>
			<version>2.13.0</version>
		</dependency>

		<dependency>
			<groupId>org.uncommons</groupId>
			<artifactId>reportng</artifactId>
			<version>1.1.4</version>
			<scope>test</scope>
			<exclusions>
				<exclusion>
					<groupId>org.testng</groupId>
					<artifactId>testng</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<!-- ISTF Artifacts END -->

		<!-- PEF Artifacts START -->
